@article{Jared,
title = {Solving Navier–Stokes with mimetic operators},
journal = {Computers & Fluids},
volume = {254},
pages = {105817},
year = {2023},
issn = {0045-7930},
doi = {10.1016/j.compfluid.2023.105817},
url = {https://doi.org/10.1016/j.compfluid.2023.105817},
author = {Jared Brzenski and Jose E. Castillo},
keywords = {Mimetic, Finite, Difference, Operators, Curvilinear},
abstract = {We present a new scheme for solving Navier–Stokes equations using mimetic difference operators. These operators can be constructed to high orders of accuracy and maintain the physical properties of the problem under consideration. We demonstrate the effectiveness of our scheme by modeling a lock release in 3D Cartesian coordinates, then extend our techniques to 3D curvilinear grids. The resulting scheme allows for simple and efficient computation of fluid processes on curvilinear grids, which allows us to solve problems in more complex regions while minimizing the restrictions of finite difference methods.}
}
